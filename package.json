{
    "name": "@nestjs-library/crud",
    "version": "0.1.0",
    "description": "Automatically generate CRUD Rest API based on NestJS and TypeOrm",
    "homepage": "https://github.com/woowabros/nestjs-library-crud",
    "repository": {
        "type": "git",
        "url": "https://github.com/woowabros/nestjs-library-crud"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "engines": {},
    "license": "MIT",
    "keywords": [
        "NestJS",
        "CRUD",
        "TypeORM",
        "REST API",
        "Swagger",
        "Decorator"
    ],
    "files": [
        "dist",
        "README.md"
    ],
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "scripts": {
        "prepare": "husky install",
        "precommit": "lint-staged",
        "type-check": "tsc -p tsconfig.lib.json --noEmit && tsc -p tsconfig.spec.json",
        "format-check": "prettier --check .",
        "format": "prettier --write --list-different .",
        "lint": "eslint --ext .ts,.js .",
        "test": "jest --config=./jest.config.ts",
        "build": "tsc -p tsconfig.lib.json",
        "release": "release-it"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "dependencies": {
        "@nestjs/common": "^9.2.1",
        "@nestjs/core": "^9.2.1",
        "@nestjs/platform-express": "^9.0.0",
        "@nestjs/swagger": "^6.1.4",
        "@nestjs/typeorm": "^9.0.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "express": "^4.18.2",
        "lodash": "^4.17.21",
        "qs": "6.11.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.0",
        "tslib": "^2.4.1",
        "typeorm": "^0.3.11"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.4.2",
        "@commitlint/config-conventional": "^17.4.2",
        "@commitlint/types": "^17.4.0",
        "@nestjs/testing": "^9.0.0",
        "@types/express": "^4.17.16",
        "@types/jest": "28.1.1",
        "@types/lodash": "^4.14.191",
        "@types/node": "18.7.1",
        "@types/qs": "^6.9.7",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.50.0",
        "@typescript-eslint/parser": "^5.50.0",
        "commitizen": "^4.3.0",
        "commitlint": "^17.4.2",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "~8.15.0",
        "eslint-config-prettier": "8.1.0",
        "eslint-import-resolver-typescript": "^3.5.2",
        "eslint-import-resolver-webpack": "^0.13.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-unicorn": "^44.0.2",
        "husky": "^8.0.0",
        "jest": "28.1.1",
        "jsonc-eslint-parser": "^2.1.0",
        "lint-staged": "^13.0.3",
        "prettier": "^2.6.2",
        "release-it": "^15.6.0",
        "sqlite3": "^5.1.2",
        "supertest": "^6.3.1",
        "ts-jest": "28.0.5",
        "ts-node": "10.9.1",
        "typescript": "~4.9.4"
    }
}
